{
  "expo": {
    "name": "GameDex",
    "slug": "GameDex-Frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gamedex.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/gamedex.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atlas004.GameDexFrontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atlas004.GameDexFrontend"
    },
    "plugins": [
      [
        "expo-build-properties", {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],

      "expo-router"
    ],
    "scheme": "your-app-scheme",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "befdd70c-469b-4c5c-8dd1-d27086d996b0"
      }
    }
  }
}

